package it.academy.cv_storage.data.utilities;



public class CustomSqlSelect {
	
	private StringBuilder strQuery = new StringBuilder();
	
	public <T> CustomSqlSelect selectFrom(Class<T> clsFrom) {
		strQuery.append(" ").append("from").append(" ")
				.append(clsFrom.getSimpleName()).append(" ");
		
		
		return this;
		
	}
	
	
	public String buildStringQuery() {
		return  strQuery.toString();
		
	}

}
