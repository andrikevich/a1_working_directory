import it.academy.cv_storage.exeption.StartSqlSentenceExeption;
import it.academy.cv_storage.model.entity.Candidate;

public class Test {
	
	public static void main(String[] args) throws StartSqlSentenceExeption {
		System.out.println(Arrays.toString(Candidate.class.getAnnotations()));
		System.out.println(Candidate.class.getSimpleName());
		
		
		CustomSqlSelect selector = new CustomSqlSelect();

		System.out.println(selector.selectAllFrom(Candidate.class).getQuery());
	}

}
